    "jest-fetch-mock": "^1.6.2",
    "jsonwebtoken": "^8.3.0",
    "lock": "^0.1.2",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.1",
    "node-fetch": "2.2.0",
    "node-uuid": "^1.4.8",
    "query-string": "^6.0.0",
    "read-text-file": "^1.1.0",
    "redux": "^3.7.2",
    "redux-promise-middleware": "^5.0.0",
    "redux-saga": "^0.16.0",
    "request": "^2.83.0",
    "restify": "^5.0.0",
    "restify-cors-middleware": "^1.1.0",
